section#friends
  h1 友邻的笔记
  ul.friends
    li.friend(ng-repeat="friend in friends")
      a(href="/#/{{ friend.uid }}")
        img(ng-src="{{ friend.small_avatar }}")
        span {{ friend.screen_name }}
    li.friend.more
      a.icon-more(href="javascript:;", ng-click="showMoreFriends()", ng-show="and_more")